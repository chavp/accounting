<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>

     <!--Import Google Icon Font-->
      <link href="http://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
      <!--Import materialize.css-->
      <link type="text/css" rel="stylesheet" href="~/Content/css/materialize.min.css"  media="screen,projection"/>
        
    <!-- 1. Load libraries -->
    <!-- IE required polyfill -->
    <script src="~/Scripts/node_modules/es6-shim/es6-shim.min.js"></script>
    <script src="~/Scripts/node_modules/angular2/es6/dev/src/testing/shims_for_IE.js"></script>

    <script src="~/Scripts/node_modules/angular2/bundles/angular2-polyfills.js"></script>
    <script src="~/Scripts/node_modules/rxjs/bundles/Rx.umd.js"></script>
    <script src="~/Scripts/node_modules/angular2/bundles/angular2-all.umd.js"></script>

    <!-- 2. Load our 'modules' -->
    <script src='~/Scripts/app/app.component.js'></script>
    <script src='~/Scripts/app/main.js'></script>

    <script src="~/Scripts/modernizr-2.6.2.js"></script>
</head>
<body>
    <nav class="container">
        <div class="nav-wrapper">
            <a href="#" class="brand-logo">#:Accounting</a>
            <ul id="nav-mobile" class="right hide-on-med-and-down">
                <li><a href="sass.html">Agile</a></li>
                <li><a href="badges.html">Scrum</a></li>
                <li><a href="badges.html">eXtream Programming</a></li>
                <li><a href="collapsible.html">Kanban</a></li>
            </ul>
        </div>
    </nav>

    <div class="container">
        @RenderBody()
        <div class="divider"></div>
        <footer class="page-footer">
            <div class="container">
                <div class="row">
                    <div class="col l6 s12">
                        <h5 class="white-text">Footer Content</h5>
                        <p class="grey-text text-lighten-4">You can use rows and columns here to organize your footer content.</p>
                    </div>
                    <div class="col l4 offset-l2 s12">
                        <h5 class="white-text">Links</h5>
                        <ul>
                            <li><a class="grey-text text-lighten-3" href="#!">Link 1</a></li>
                            <li><a class="grey-text text-lighten-3" href="#!">Link 2</a></li>
                            <li><a class="grey-text text-lighten-3" href="#!">Link 3</a></li>
                            <li><a class="grey-text text-lighten-3" href="#!">Link 4</a></li>
                        </ul>
                    </div>
                </div>
            </div>
            <div class="footer-copyright">
                <div class="container">
                    © 2014 Copyright Text
                    <a class="grey-text text-lighten-4 right" href="#!">More Links</a>
                </div>
            </div>
        </footer>
    </div>

    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <script type="text/javascript" src="~/Scripts/materialize.min.js"></script>
</body>
</html>